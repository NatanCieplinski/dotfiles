{
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "workbench.enableExperiments": false,
  "zenMode.fullScreen": false,
  /**
   * Startup, update & save
   **/
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 10000,
  "launch": {},
  "workbench.startupEditor": "welcomePage",
  /**
   * Editor appearance
   **/
  "diffEditor.wordWrap": "on",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorStyle": "line-thin",
  "editor.folding": true,
  "editor.fontSize": 15,
  "editor.glyphMargin": true,
  "editor.lineNumbers": "on",
  "editor.matchBrackets": "never",
  "editor.minimap.maxColumn": 400,
  "editor.minimap.renderCharacters": false,
  "editor.overviewRulerBorder": true,
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "none",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 120,
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#c7c8c92d",
    "editor.lineHighlightBorder": "#f7f7f71f"
  },
  "workbench.commandPalette.history": 10,
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": "single",
  "workbench.iconTheme": "vscode-seedling-icon-theme",
  "workbench.productIconTheme": "feather-vscode",
  "workbench.sideBar.location": "right",
  "workbench.statusBar.visible": true,
  /**
   * Editor behaviour
   **/
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.addMissingImports": true,
    "source.organizeImports": true
  },
  "editor.dragAndDrop": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.guides.highlightActiveIndentation": false,
  "editor.inlineSuggest.enabled": true,
  "editor.quickSuggestionsDelay": 0,
  "editor.stickyScroll.enabled": true,
  "editor.suggest.insertMode": "replace",
  "editor.suggest.localityBonus": true,
  "editor.suggest.preview": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggestSelection": "first",
  "editor.inlineSuggest.suppressSuggestions": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.splitInGroupLayout": "vertical",
  /**
   * Terminal
   **/
  "terminal.integrated.fontFamily": "Hack NF",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "100",
  "terminal.integrated.shell.osx": "/bin/zsh",
  /**
   * Find
   **/
  "search.exclude": {
    "**/build": true,
    "**/composer.lock": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true
  },
  "search.useIgnoreFiles": false,
  /**
   * Formatting & Linting
   **/
  "editor.tabSize": 3,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  /**
   * Language specifics
   **/
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "dart.flutterSdkPath": "/Library/Flutter/flutter",
  "dart.openDevTools": "flutter",
  "dart.previewLsp": true,
  "[markdown]": {
    "editor.wordWrap": "off",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "markdown.preview.breaks": true,
  "java.configuration.checkProjectSettingsExclusions": false,
  "java.errors.incompleteClasspath.severity": "ignore",
  "java.home": "/Library/Java/JavaVirtualMachines/jdk-11.0.9.jdk/Contents/Home",
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "python.autoComplete.extraPaths": [
    "/usr/local/lib/python3.8/site-packages",
    "./"
  ],
  "python.pythonPath": "/usr/bin/python3",
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  /**
   * Extensions
   **/
  "extensions.ignoreRecommendations": true,
  "autoDarkMode.darkTheme": "GitHub Dark",
  "autoDarkMode.lightTheme": "GitHub Light",
  "branchwarnings.protectedBranches": [
    "deploy",
    "main",
    "master",
    "prod",
    "development"
  ],
  "errorLens.gutterIconsEnabled": true,
  "errorLens.enabledDiagnosticLevels": ["error", "info", "hint", "warning"],
  "eslint.format.enable": true,
  "gitlens.blame.dateFormat": "ddd k:m, MMM Do, YYYY",
  "gitlens.blame.highlight.locations": ["gutter", "overview", "line"],
  "indenticator.hover.peekBack": 0,
  "indenticator.inner.showIndentGuide": true,
  "indenticator.showCurrentDepthInStatusBar": false,
  "indenticator.showIndentGuide": false,
  "path-intellisense.autoTriggerNextSuggestion": true,
  "peacock.affectActivityBar": false,
  "peacock.affectTitleBar": false,
  "projectManager.git.baseFolders": ["~/Developer/"],
  "tailwindCSS.lint.cssConflict": "error",
  "tailwindCSS.emmetCompletions": true,
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.filtering.excludeGlobs": ["**/node_modules/*"],
  "todo-tree.general.enableFileWatcher": true,
  "todo-tree.general.tags": ["TODO", "FIXME", "HACK", "TYPE"],
  "todo-tree.highlights.foregroundColourScheme": ["white"],
  "todo-tree.highlights.highlightDelay": 0,
  "todo-tree.highlights.useColourScheme": true,
  "todo-tree.highlights.backgroundColourScheme": [
    "black",
    "red",
    "orange",
    "teal",
    "violet",
    "indigo",
    "brown"
  ],
  "volar.codeLens.scriptSetupTools": true,
  "volar.completion.preferredAttrNameCase": "camel",
  "auto-close-tag.disableOnLanguage": ["typescript"],
  "vscode-pets.petSize": "medium",
  "projectManager.confirmSwitchOnActiveWindow": "always",
  "projectManager.openInNewWindowWhenClickingInStatusBar": true,
  "githubPullRequests.pullBranch": "never",
  "findUnusedExports.defaultResultExpanded": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "yaml": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.definitionLinkOpensInPeek": true,
  "workbench.colorTheme": "GitHub Light",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "window.commandCenter": false,
  "chat.editor.fontSize": 14,
  "nxConsole.showNodeVersionOnStartup": false,
  "git.autofetch": true,
  "window.zoomLevel": -1
}